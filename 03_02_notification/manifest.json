{
    "manifest_version": 3,
    "name": "notification-extension",
    "version": "0.1",
  
    "description": "これは通知を検証するブラウザ拡張機能です。",
  
    "icons": {
      "32": "icons/check_box_32dp.png"
    },

    "background": {
      "scripts": ["background.js"]
    },

    "permissions": [
      "notifications",
      "menus"
    ]
}